# Staging Environment Variables
# Copy this to .env.staging and fill in your staging Firebase values

# Staging Firebase Configuration
VITE_FIREBASE_API_KEY_STAGING=your_staging_api_key_here
VITE_FIREBASE_SENDER_ID_STAGING=your_staging_sender_id_here
VITE_FIREBASE_APP_ID_STAGING=your_staging_app_id_here

# Environment Indicator
VITE_ENVIRONMENT=staging
VITE_APP_NAME="AI Agent Hub - Staging"

# Feature Flags
VITE_ENABLE_DEBUG_LOGGING=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=false
